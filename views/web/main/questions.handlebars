<section class="header-title">
    <div class="container">
        <div class="row">
            <div class="title">
                <h2>Sorular</h2>
            </div>
        </div>
    </div>
</section>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="search-box">
                        <div class="form-group">

                            <input class="form-control" placeholder="Ara" id="searchInput" oninput="newQuestionSearch();"
                                type="input" />
                        </div>
                    </div>
            <div id="accordion" role="tablist">
                {{#each question}}
                <div class="toggle">
                    <div class="toggle-heading" role="tab">
                        <a class="question" href="/sorular/{{_id}}"> <i class="fa fa-question"></i>{{questionName}}</a>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>